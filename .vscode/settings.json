{
    "folders": [
        {
            "path": "."
        }
    ],
    "extensions": {
        "recommendations": [
            "platformio.platformio-ide",
            "ms-vscode.cpptools",
            "ms-vscode.cpptools-extension-pack"
        ]
    },
    "settings": {
        "files.associations": {
            "*.h": "cpp",
            "*.cpp": "cpp",
            "*.ino": "cpp"
        },
        "C_Cpp.errorSquiggles": "Enabled",
        "C_Cpp.intelliSenseEngine": "Default"
    }
}